{
  "name": "quickstart-express-react",
  "version": "1.0.0",
  "description": "Multi-page application quickstart for Express + React",
  "scripts": {
    "start": "bash -c 'NODE_ENV=production node index.js'",
    "clean": "bash -c 'rm -rf public/{js,css}'",
    "dev:js": "node index dev",
    "dev:css": "gulp scss:watch",
    "dev": "npm-run-all clean -p dev:*",
    "build:js": "webpack",
    "build:css": "gulp scss:build",
    "compress": "bash -c 'gzip -9 -q public/**/*.{css,js,map}'",
    "build": "npm-run-all clean -p build:* -s compress",
    "tool": "node index",
    "db:init": "npm run db:migrate:rollback && npm run db:migrate:latest && npm run db:seed",
    "db:migrate:rollback": "./node_modules/.bin/knex --knexfile ./server/database/knexfile.js migrate:rollback",
    "db:migrate:latest": "./node_modules/.bin/knex --knexfile ./server/database/knexfile.js migrate:latest",
    "db:seed": "./node_modules/.bin/knex --knexfile ./server/database/knexfile.js seed:run"
  },
  "keywords": [
    "quickstart",
    "express",
    "react"
  ],
  "author": "Ionut-Cristian Florescu <ionut.florescu@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-latest-minimal": "^1.1.2",
    "bluebird": "^3.4.7",
    "body-parser": "^1.17.2",
    "bookshelf": "^0.10.3",
    "browser-sync": "^2.18.12",
    "compression": "^1.6.2",
    "config": "^1.26.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.3",
    "express-validator": "^3.2.0",
    "joi": "^10.6.0",
    "knex": "^0.13.0",
    "lowdb": "^0.14.0",
    "mime": "^1.3.4",
    "morgan": "^1.8.2",
    "pug": "^2.0.0-rc.2",
    "sqlite3": "^3.1.8",
    "tcomb-validation": "^3.3.0",
    "underscore-db": "^0.12.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "axios": "^0.16.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-sync": "^2.18.12",
    "classnames": "^2.2.5",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "gulp": "^3.9.1",
    "gulp-postcss": "^7.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "material-ui": "^0.18.3",
    "moment": "^2.18.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
